        </tbody>
    </table>
</div>
<div id="footer">
    <script type="text/javascript">
        $('.dTable').dataTable({
            "bJQueryUI": true,
            "sPaginationType": "full_numbers",
            "sDom": '<""l>t<"F"fp>'
        });
    </script>
</div>